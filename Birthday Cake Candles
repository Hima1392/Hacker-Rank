#include<bits/stdc++.h>
using namespace std;
int main()
{
    int arr[100000];
    int n,mx=0,cnt=0;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
        if(arr[i]>mx)
            mx=arr[i];
    }
    for(int i=0;i<n;i++)
    {
        if(arr[i]==mx)
            cnt++;
    }
    cout<<cnt;
}
