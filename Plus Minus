#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,a[1000];
    scanf("%d",&n);
    int i;
    float ps=0,ng=0,zr=0;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        if(a[i]>0)ps++;
        else if(a[i]<0)ng++;
        else zr++;
    }
    printf("%f\n%f\n%f",ps/n,ng/n,zr/n);

return 0;
}

