#include<bits/stdc++.h>
using namespace std;
int main()
{
    int a[10],b[10];
    int i;
    for(i=0;i<3;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<3;i++)
    {
        scanf("%d",&b[i]);
    }
    int p[]={0,0};
    for(i=0;i<3;i++)
    {
        if(a[i]>b[i])p[0]++;
        else if(a[i]<b[i])p[1]++;
    }
    printf("%d %d",p[0],p[1]);
return 0;
}

